{
  "firebase": "angular-fire-2",
  "public": "~/Public",
  "redirects": [
    {
      "source": "/",
      "destination": "/api",
      "type": 302
    }
  ],
  "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
  "rewrites": [
    {
      "source": "**",
      "destination": "/index.html"
    }
  ]
}
